        "prisma": "^5.22.0"
    },
    "devDependencies": {
        "@types/cors": "^2.8.19",
        "@types/express": "^5.0.6",
        "@types/node": "^25.0.3",
        "nodemon": "^3.0.2",
        "ts-node": "^10.9.2",
        "typescript": "^5.9.3"
    }
