{
    "name": "server",
    "version": "1.0.0",
    "description": "Minimal API server for Student Dashboard",
    "main": "dist/index.js",
    "scripts": {
        "start": "node dist/index.js",
        "server": "ts-node index.ts",
        "build": "npx prisma generate && tsc",
        "postinstall": "npm run build",
        "init-db": "ts-node scripts/init-db.ts",
